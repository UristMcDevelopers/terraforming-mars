function init(self)
	self.player_list = { { end_his_round = false, avalibale_actions = {true, true} } }
	self.player_whos_turn = 1
	self.round = 0
	self.is_ended = false
end

function on_message(self, message_id, message, sender)
	if message_id == hash("player_action") then
		print("player_action ", sender)
	end
end

function on_end(self)
	self.is_ended = true
end	

